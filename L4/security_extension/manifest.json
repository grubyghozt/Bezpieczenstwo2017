{
  "name": "Security assignment interceptor",
  "description": "Intercept inputs",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "tabs",
    "notifications",
    "http://*/",
    "https://*/"
  ],
  "background": {
    "scripts": ["jquery-3.2.1.min.js", "background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Make this page red"
  },
  "manifest_version": 2
}
